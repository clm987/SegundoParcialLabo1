#ifndef ARTICULO_H_
#define ARTICULO_H_

typedef struct{
	int id_rubro;
	char nombre_rubro[30];
}eRubro;

typedef struct{
	int Id_Articulo;
	char articulo[128];
	char medida[128];
	float precio;
	eRubro rubro;
}eArticulo;





#endif /* ARTICULO_H_ */
